mov ah, 0x0e
mov al, 'A'

mov bl, ('A' - 'a')

loop:
    ; printing uppercase
    int 0x10
    ; printing lowercase
    sub al, bl
    int 0x10

    ;return to uppercase
    add al, bl

    inc al ; add al, 1
    cmp al, 'Z'
    jle loop
    jmp exit


exit:
    jmp $

times 510-($-$$) db 0
db 0x55, 0xaa

